{
  "name": "youtube-playlists-under-video",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "yths-server",
    "yths-ext"
  ],
  "scripts": {
    "dev": "concurrently -k -n SERVER,REACT,EXT -c blue,green,magenta \"yarn dev-server\" \"yarn dev-vitebuild\" \"yarn dev-ext\"",
    "dev-sim": "concurrently -k -n SERVER,REACT -c blue,green \"yarn dev-server\" \"yarn dev-vite\"",
    "dev-ite": "yarn workspace yths-ext run dev-vite",
    "dev-vitebuild": "yarn workspace yths-ext run dev-vitebuild",
    "dev-ext": "yarn workspace yths-ext run dev-ext",
    "dev-server": "yarn workspace yths-server run dev",
    "build-server": "yarn workspace yths-server run build",
    "build-vite": "yarn workspace yths-ext run build-vite"
  }
}
